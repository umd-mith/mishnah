mishnahw_pc =
  
  ##  (punctuation character) contains a character or string of characters regarded as constituting a
  ##   single punctuation mark. [17.1. ]
  element pc {
    (text
     | mishnahw_model.gLike
     | mishnahw_c
     | mishnahw_model.pPart.edit)*,
