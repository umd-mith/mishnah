            <witness xml:id="S01068" n="T" corresp="S01068.xml">London MS</witness>
            <witness xml:id="S07055" n="T" corresp="S07055.xml">Erfurt MS</witness>
          </listWit>
        </listWit>

      </sourceDesc>
    </fileDesc>
    <encodingDesc>
